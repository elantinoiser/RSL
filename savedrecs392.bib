@inproceedings{ ISI:000404963800019,
Author = {Taylor, Rocky S.},
Editor = {{Ulengin, F and Li, K and Boltze, M}},
Title = {{Ice-related Disruptions to Ferry Services in Eastern Canada: Prevention
   and Consequence Mitigation Strategies}},
Booktitle = {{WORLD CONFERENCE ON TRANSPORT RESEARCH - WCTR 2016}},
Series = {{Transportation Research Procedia}},
Year = {{2017}},
