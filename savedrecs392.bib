@article{ ISI:000461410400001,
Author = {Zhou, Yaoming and Wang, Junwei and Sheu, Jiuh-Biing},
Title = {{On connectivity of post-earthquake road networks}},
Journal = {{TRANSPORTATION RESEARCH PART E-LOGISTICS AND TRANSPORTATION REVIEW}},
Year = {{2019}},
Volume = {{123}},
Pages = {{1-16}},
Month = {{MAR}},
Abstract = {{This paper proposes a novel stochastic mathematical framework to
   investigate the connectivity of road networks impacted by earthquakes.
   The concepts of ``global connectivity{''} and ``local connectivity{''}
   are defined and evaluated using percolation theory. Specifically, global
   connectivity measures the extent to which the whole network is
   connected, and local connectivity measures the distances between each
   node to its neighbors. Furthermore, the concept of efficiency is
   employed to integrate local connectivity. A new percolation process
   integrating the traditional localized attack and random failure is
   proposed, which sheds light on the application of percolation theory to
   practical networks when facing disruptions.}},
