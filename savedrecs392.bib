@article{ ISI:000388784600009,
Author = {Gras, Maria-Eugenia and Font-Mayolas, Silvia and Patino, Josefina and
   Baltasar, Alicia and Planes, Montserrat and Sullman, Mark J. M.},
Title = {{Resilience and the expression of driving anger}},
