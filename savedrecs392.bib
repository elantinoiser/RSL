@inproceedings{ ISI:000383251001078,
Author = {Turan, Osman and Kurt, Rafet Emek and Arslan, Volkan and Silvagni, Sara
   and Ducci, Marco and Liston, Paul and Schraagen, Jaan Maarten and Fang,
   Ivy and Papadakis, George},
