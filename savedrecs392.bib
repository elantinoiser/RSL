@article{ ISI:000372390000002,
Author = {Hasani, Aliakbar and Khosrojerdi, Amirhossein},
Title = {{Robust global supply chain network design under disruption and
   uncertainty considering resilience strategies: A parallel memetic
   algorithm for a real-life case study}},
Journal = {{TRANSPORTATION RESEARCH PART E-LOGISTICS AND TRANSPORTATION REVIEW}},
Year = {{2016}},
Volume = {{87}},
Pages = {{20-52}},
Month = {{MAR}},
Abstract = {{A mixed-integer, non-linear model is developed for designing robust
   global supply chain networks under uncertainty. Six resilience
   strategies are proposed to mitigate the risk of correlated disruptions.
   In addition, an efficient parallel Taguchi-based memetic algorithm is
   developed that incorporates a customized hybrid parallel adaptive large
   neighborhood search. Fitness landscape analysis is used to determine an
   effective selection of neighborhood structures, while the upper bound
   found by Lagrangian relaxation heuristic is used to evaluate quality of
   solutions and effectiveness of the proposed metaheuristic. The model is
   solved for a real-life case of a global medical device manufacturer to
   extract managerial insights. (C) 2015 Elsevier Ltd. All rights reserved.}},
