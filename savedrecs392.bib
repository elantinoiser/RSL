@article{ ISI:000430897900016,
Author = {Liu, Chiung-Lin and Shang, Kuo-Chung and Lirn, Taih-Cherng and Lai,
   Kee-Hung and Lun, Y. H. Venus},
Title = {{Supply chain resilience, firm performance, and management policies in
   the liner shipping industry}},
Journal = {{TRANSPORTATION RESEARCH PART A-POLICY AND PRACTICE}},
Year = {{2018}},
Volume = {{110}},
Pages = {{202-219}},
Month = {{APR}},
Note = {{International Forum of Shipping, Ports and Airports (IFSPA), Hong Kong,
   HONG KONG, NOV 29-DEC 02, 2015}},
Organization = {{Hong Kong Polytechn Univ, Dept Logist \& Maritime Studies}},
Abstract = {{This study empirically examines a model that describes the relationship
   between supply chain resilience (SCR) and firm performance by using
   survey data collected from the Taiwanese liner shipping industry. In the
   model, the theoretical constructs of SCR consist of a risk management
   culture, agility, integration, and supply chain (re-)engineering. The
   results from testing the model show that the positive direct effects of
   a risk management culture on agility, integration, and supply chain
   (re-)engineering are significant and that risk management performance
   contributes to firm performance. The findings also suggest that risk
   management performance plays a crucial role in the positive effects of
   the three types of SCR (i.e., agility, integration, and supply chain
   (re-)engineering) on firm performance. Managers are advised to focus on
   the role of risk management performance to realize the performance value
   of SCR. (C) 2017 Elsevier Ltd. All rights reserved.}},
