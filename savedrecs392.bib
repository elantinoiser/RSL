@article{ ISI:000304892200007,
Author = {Beevers, Lindsay and Douven, Wim and Lazuardi, Helmi and Verheij, Henk},
Title = {{Cumulative impacts of road developments in floodplains}},
Journal = {{TRANSPORTATION RESEARCH PART D-TRANSPORT AND ENVIRONMENT}},
Year = {{2012}},
Volume = {{17}},
Number = {{5}},
Pages = {{398-404}},
Month = {{JUL}},
Abstract = {{This paper investigates the impact of differing road development
   strategies in floodplains for increasing levels of road expansion,
   focusing on hydraulic characteristics of floods such as water level,
   velocity and inundation duration and extent. Cumulative impacts are
   presented in term of both a resistant approach where road transport
   infrastructure has the secondary purpose of flood dykes and, a
   resilience approach that maintains the floodplain hydraulics through the
   use of flow through structures. Each method is examined in the context
   of road development in the Cambodian Mekong floodplain. Results indicate
   that resistance approaches necessitate higher levels of road structures,
   designed to higher technical specifications, whilst resilience
   approaches maintain the hydraulic character of floodplains but require
   the inclusion of well-designed flow through structures with localized
   scour protection. (C) 2012 Published by Elsevier Ltd.}},
