@inproceedings{ ISI:000314227500005,
Author = {Meijer, Sebastiaan},
Editor = {{Papaioannou, P}},
Title = {{Introducing gaming simulation in the Dutch railways}},
Booktitle = {{TRANSPORT RESEARCH ARENA 2012}},
