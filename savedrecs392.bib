@article{ ISI:000404703200003,
Author = {Gaston Cedillo-Campos, Miguel and Lizarraga-Lizarraga, Giovanni and
   Daniel Martner-Peyrelongue, Carlos},
Title = {{MiF3 method: Modeling intermodal fluidity freight flows}},
Journal = {{RESEARCH IN TRANSPORTATION ECONOMICS}},
