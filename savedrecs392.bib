@article{ ISI:000352176400010,
Author = {Yang, Yuefeng and Xu, Xuerong},
Title = {{Post-disaster grain supply chain resilience with government aid}},
Journal = {{TRANSPORTATION RESEARCH PART E-LOGISTICS AND TRANSPORTATION REVIEW}},
Year = {{2015}},
Volume = {{76}},
Pages = {{139-159}},
Month = {{APR}},
Abstract = {{Assessing the disruption and resilience of the agricultural grain supply
   chain is critical to ensure grain supply and stabilize grain price in
   the final market. This research proposes a quantitative model to analyze
   how a grain processor regains robustness when supply is disrupted by a
   natural disaster upstream, and how this disruption affects grain
   retailers downstream. Two supply chain recovery methods, contingent
   sourcing and government aid, are considered for grain processor
   recovery. The results show that (1) a processor prefers timely full
   recovery, and (2) government aid as an intervention means is
   indispensable but cannot fully replace the backup supplier. (C) 2015
   Elsevier Ltd. All rights reserved.}},
