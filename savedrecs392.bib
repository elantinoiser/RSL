@article{ ISI:000441989200007,
Author = {Carlos Garcia-Palomares, Juan and Gutierrez, Javier and Carlos Martin,
   Juan and Moya-Gomez, Borja},
Title = {{An analysis of the Spanish high capacity road network criticality}},
Journal = {{TRANSPORTATION}},
Year = {{2018}},
Volume = {{45}},
