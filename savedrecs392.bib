@article{ ISI:000489318100050,
Author = {Ahmed, Shofiq and Dey, Kakan and Fries, Ryan},
Title = {{Evaluation of Transportation System Resilience in the Presence of
   Connected and Automated Vehicles}},
Journal = {{TRANSPORTATION RESEARCH RECORD}},
